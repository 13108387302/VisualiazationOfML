    "demo_project": {
      "name": "示例项目",
      "description": "机器学习基础流程示例",
      "components": [
        {
          "id": "data_loader_1",
          "type": "data",
          "name": "数据加载",
          "position": [100, 100],
          "properties": {
            "file_path": "data.csv",
            "separator": ",",
            "encoding": "utf-8"
          }
        },
        {
          "id": "preprocessor_1",
          "type": "preprocess",
          "name": "标准化",
          "position": [300, 100],
          "properties": {
            "method": "Z-score"
          }
        },
        {
          "id": "model_1",
          "type": "model",
          "name": "随机森林",
          "position": [500, 100],
          "properties": {
            "n_estimators": 100,
            "max_depth": 10,
            "random_state": 42
          }
        }
      ],
      "connections": [
        {
          "start_component": "data_loader_1",
          "start_port": 0,
          "end_component": "preprocessor_1",
          "end_port": 0
        },
        {
          "start_component": "preprocessor_1",
          "start_port": 0,
          "end_component": "model_1",
          "end_port": 0
        }
      ]
    }
  },
  "performance_limits": {
    "max_components": 1000,
